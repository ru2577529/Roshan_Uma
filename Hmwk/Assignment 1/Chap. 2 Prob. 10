 */

#include <iostream>

using namespace std;

int main(int argc, char** argv) {

    double carGallons = 15;
    double carRange = 375;
    double mpG = carRange / carGallons;
    
    cout << "The car gets " << mpG << "MPG." << endl;
    
    return 0;
}

