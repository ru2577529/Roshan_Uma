 * Purpose: To calculate acres within a set square footage.
 */

#include <iostream>

using namespace std;

int main(int argc, char** argv) {

    double oneAcre = 43560;
    double manyAcre = 391876 / oneAcre;
    
    cout << "There are " << manyAcre << " acres in 391,876 feet" << endl;
    
    return 0;
}

